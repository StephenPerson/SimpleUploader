<div class="media">
  <div class="media-left">
  	<figure class="imghvr-blur">
    	<src <%= link_to image_tag(photo.image.url, class: 'animage')%> </src>
    	<figcaption>
    		<center><h4 class="media-heading"><%= photo.title %></h4></center>
    		<%= link_to '', photo_path(photo), method: :delete, data: {confirm: 'Click OK to delete'} %>
    	</figcaption>
	</figure>
  </div>
</div>